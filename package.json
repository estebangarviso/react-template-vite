{
	"$schema": "https://json.schemastore.org/package.json",
	"version": "0.0.1",
	"project": "(((project-name)))",
	"name": "(((app-name)))",
	"title": "(((app-title)))",
	"description": "(((app-description)))",
	"scripts": {
		"start:dev": "env -e dev -m debug --log debug : vite",
		"start:prod": "env -e prod -m debug : vite",
		"build:dev": "env -e dev -m build --log debug : vite build",
		"build:prod": "env -e dev -m build --log debug : vite build",
		"test:dev": "env -e dev -m test : vitest",
		"test:prod": "env -e prod -m test : vitest",
		"test:mutation": "env -e dev -m test : stryker run",
		"env:schema": "env schema -e dev -m build",
		"lint": "eslint --cache --cache-location node_modules/.cache/eslint/",
		"stylelint": "stylelint --cache src/**/*.css --cache-location node_modules/.cache/stylelint/",
		"format": "prettier --cache --write **/*.{ts,cts,mts,tsx,css,json,html,md,yml,yaml,webmanifest}",
		"clean": "pnpx del-cli dist reports **/*.tsbuildinfo .swc .node_modules",
		"preview": "pnpm build:dev & env -e dev -m build : vite preview",
		"prepare": "node .husky/install.mjs"
	},
	"imports": {
		"#libs/ioc": "./src/libs/ioc/ioc.builder.tsx",
		"#libs/router": "./src/libs/router/index.ts",
		"#libs/feature": "./src/libs/feature/index.ts"
	},
	"dependencies": {
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-router": "7.6.3",
		"zustand": "4.5.2"
	},
	"devDependencies": {
		"@commitlint/cli": "19.3.0",
		"@commitlint/config-conventional": "19.2.2",
		"@commitlint/types": "19.0.3",
		"@iconify-json/mdi": "^1.2.3",
		"@stryker-mutator/core": "9.0.1",
		"@stryker-mutator/typescript-checker": "9.0.1",
		"@stryker-mutator/vitest-runner": "9.0.1",
		"@testing-library/dom": "10.4.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "16.3.0",
		"@testing-library/user-event": "14.6.1",
		"@types/eslint": "9.6.1",
		"@types/node": "24.0.5",
		"@types/react": "19.1.8",
		"@types/react-dom": "19.1.6",
		"@unocss/eslint-config": "66.3.2",
		"@v10e/env": "2.12.0",
		"@vitejs/plugin-react-swc": "3.10.2",
		"@vitest/coverage-v8": "3.2.4",
		"eslint": "9.29.0",
		"eslint-config-prettier": "10.1.5",
		"eslint-plugin-jest-dom": "^5.5.0",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-perfectionist": "4.15.0",
		"eslint-plugin-prettier": "5.5.1",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-redos": "4.4.5",
		"eslint-plugin-regexp": "2.9.0",
		"eslint-plugin-sonarjs": "3.0.4",
		"eslint-plugin-testing-library": "7.5.3",
		"eslint-plugin-tsdoc": "0.4.0",
		"eslint-plugin-unicorn": "59.0.1",
		"eslint-plugin-vitest": "0.5.4",
		"globals": "16.2.0",
		"happy-dom": "^18.0.1",
		"husky": "9.0.11",
		"postcss": "8.5.6",
		"prettier": "3.6.2",
		"stylelint": "16.21.0",
		"stylelint-config-rational-order": "0.1.2",
		"stylelint-config-recommended": "16.0.0",
		"stylelint-config-standard": "38.0.0",
		"stylelint-config-tailwindcss": "^1.0.0",
		"stylelint-order": "7.0.0",
		"stylelint-prettier": "5.0.3",
		"stylelint-use-nesting": "^6.0.0",
		"tslib": "2.8.1",
		"typescript": "5.8.3",
		"typescript-eslint": "8.35.0",
		"unocss": "66.3.2",
		"vite": "7.0.0",
		"vite-plugin-checker": "0.9.3",
		"vite-plugin-pwa": "1.0.0",
		"vite-plugin-webfont-dl": "3.10.5",
		"vitest": "3.2.4",
		"vitest-mock-extended": "3.1.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@parcel/watcher",
			"@swc/core",
			"esbuild"
		]
	},
	"type": "module",
	"engines": {
		"node": ">=22",
		"npm": ">=10",
		"pnpm": ">=10"
	},
	"license": "MIT",
	"repository": "https://github.com/estebangarviso/react-vite-starter",
	"keywords": [
		"react",
		"pnpm",
		"vite",
		"vitest",
		"typescript",
		"spa",
		"pwa",
		"unocss"
	]
}
