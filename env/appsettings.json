{
	"$schema": "../node_modules/@calvear/env/schemas/env.schema.json",
	"|DEFAULT|": {
		"APP_ENV": "[[ENV]]",
		"APP_NAME": "[[NAME]]",
		"APP_VERSION": "[[VERSION]]",
		"BASE_URL": "/",
		"FONT_FAMILY": "Quicksand",
		"FONT_WEIGHTS": "300..700",
		"SERVICE_WORKER": false,
		"APP_NO_SCRIPT": "You need to enable JavaScript to run this app."
	},
	"|MODE|": {
		"build": {
			"NODE_ENV": "production",
			"BASE_URL": "/(((base-path)))"
		},
		"debug": {
			"NODE_ENV": "development",
			"PORT": 3003
		},
		"test": {
			"NODE_ENV": "test"
		}
	},
	"|ENV|": {
		"dev": {},
		"release": {}
	},
	"|LOCAL|": {
		"dev": {},
		"release": {}
	}
}
