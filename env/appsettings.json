{
	"$schema": "../node_modules/@calvear/env/schemas/env.schema.json",
	"|DEFAULT|": {
		"BASE_URL": "/",
		"FONT_FAMILY": "Lato",
        "FONT_WEIGHTS": "200;400;600;800",
		"SERVICE_WORKER": false,
		"APP": {
			"ENV": "[[ENV]]",
			"NAME": "[[NAME]]",
			"VERSION": "[[VERSION]]",
			"NO_SCRIPT": "You need to enable JavaScript to run this app."
		}
	},
	"|MODE|": {
		"build": {
			"NODE_ENV": "production",
			"BASE_URL": "/(((base-path)))"
		},
		"debug": {
			"NODE_ENV": "development",
			"PORT": 3003
		},
		"test": {
			"NODE_ENV": "test"
		}
	},
	"|ENV|": {
		"dev": {},
		"prod": {}
	},
	"|LOCAL|": {
		"dev": {},
		"prod": {}
	}
}

